    "next-mdx-remote": "^2.1.3",
    "postcss": "^8.2.4",
    "react": "17.0.1",
    "react-dark-mode-toggle": "^0.0.10",
    "react-dom": "17.0.1",
    "react-icons": "^4.1.0",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.0",
